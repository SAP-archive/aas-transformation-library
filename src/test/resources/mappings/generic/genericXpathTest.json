{
	"@header": {
		"version": "1.0.0",
		"aasVersion": "3.0RC01",
		"@namespaces": {
			"ns": "http://ns.org/"
		},
		"placeholders": [
			{
				"name": "genericPlaceholder",
				"description": "A generic placeholder"
			}
		]
	},
	"aasEnvironmentMapping": {
		"submodels": [
			{
				"@foreach": {
					"@xpath": "/ns:root/ns:a"
				},
				"@bind": {
					"idShort": {
						"@xpath": "@name"
					}
				},
				"submodelElements": [
					{
						"@foreach": {
							"@xpath": "ns:b"
						},
						"modelType": "Property",
						"value": "${{genericPlaceholder}}",
						"@bind": {
							"idShort": {
								"@xpath": "@name"
							}
						}
					}
				]
			}
		]
	}
}