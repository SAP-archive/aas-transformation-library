{
  "@header": {
    "aasVersion": "3.0RC01",
    "@parameters": {
      "assetName": "Name of the asset",
      "submodelName": "Name of the submodel"
    }
  },
  "aasEnvironmentMapping": {
    "assetAdministrationShells": [
      {
        "@foreach": {
          "@xpath": "//caex:SystemUnitClass"
        },
        "assetInformation": {
          "@bind": {
            "assetKind": {
              "@xpath": "TYPE"
            }
          },
          "identification": {
            "keys": [
              {
                "id": "assetInformationId",
                "idType": "Custom"
              }
            ]
          },
          "globalAssetId": {
            "value": "assetInformationGlobalAssetIdReference",
            "idType": "CUSTOM",
            "type": "ASSET"
          }
        }
      }
    ],
    "submodels": [
      {
        "@foreach": {
          "@caexAttributeName": "IdentificationData"
        },
        "idShort": "http://sap.com",
        "identification": {
          "@bind": {
            "id": {
              "@concatenate": [
                "submodel ",
                {
                  "@param": "submodelName"
                },
                " of asset ",
                {
                  "@param": "assetName"
                }
              ]
            }
          }
        },
        "submodelElements": []
      }
    ],
    "conceptDescriptions": [
    ]
  },
  "configMappings": [
    {
      "submodels": [
        {
          "from_attributeName": "IdentificationData",
          "semanticId": "http://sap.com",
          "idGeneration": {
            "parameters": [
              {
                "from_string": "submodel ${{submodelName}} of asset ${{assetName}}"
              }
            ]
          },
          "submodelElements": []
        }
      ]
    }
  ]
}
